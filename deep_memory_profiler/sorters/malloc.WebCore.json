{
  "type": "sorter",
  "version": 1,
  "world": "malloc",
  "name": "blink",
  "order": {},
  "rules": [
    {
      "name": "RenderStyle",
      "backtrace_function": ".*blink::RenderStyle::(create|clone).*"
    },
    {
      "name": "SharedBuffer",
      "backtrace_function": ".*blink::SharedBuffer::(create|buffer|append).*"
    },
    {
      "name": "XMLHttpRequest",
      "backtrace_function": ".*blink::XMLHttpRequest::create .*"
    },
    {
      "name": "XMLHttpRequest-received",
      "backtrace_function": ".*blink::XMLHttpRequest::didReceiveData.*"
    },
    {
      "name": "DocumentWriter-addData",
      "backtrace_function": ".*blink::DocumentWriter::addData.*"
    },
    {
      "name": "Element-create",
      "backtrace_function": ".*blink::(HTML[A-Za-z0-9_]*Element|Text|Comment|HTMLDocument|CSSStyleRule|Attribute|DOMWindow)::create .*"
    },
    {
      "name": "Element-factory",
      "backtrace_function": ".*blink::HTML[a-zA-Z0-9_]*Factory::create[a-zA-Z0-9_]*Element.*"
    },
    {
      "name": "ElementWrapper",
      "backtrace_function": ".*blink::createHTML[a-zA-Z0-9_]*ElementWrapper.*"
    },
    {
      "name": "StylePropertySet",
      "backtrace_function": ".*blink::StylePropertySet::create .*"
    },
    {
      "name": "StyleElement-createSheet",
      "backtrace_function": ".*blink::StyleElement::createSheet.*"
    },
    {
      "name": "Resource",
      "backtrace_function": ".*blink::ResourceFetcher::requestResource .*"
    },
    {
      "name": "ScriptLoader-execute",
      "backtrace_function": ".*blink::ScriptLoader::execute.*"
    },
    {
      "name": "Events",
      "backtrace_function": ".*blink::(createAttributeEventListener|V8LazyEventListener::create|V8EventListener::create|Event::create |EventListener::create ).*"
    },
    {
      "name": "Document-write",
      "backtrace_function": ".*blink::Document::write.*"
    },
    {
      "name": "Element-createRenderer",
      "backtrace_function": ".*blink::[a-zA-Z0-9_]*Element::createRenderer .*"
    },
    {
      "name": "ImageFrameGenerator",
      "stacktrace": ".*blink::ImageFrameGenerator.*"
    },
    {
      "name": "Render",
      "backtrace_function": ".*blink::(RenderLayer|RenderBlock|RenderWidget|RenderView|RenderViewImpl|RenderStyle|RenderText).*"
    },
    {
      "name": "setInnerHTML",
      "backtrace_function": ".*blink::Element::setInnerHTML.*"
    },
    {
      "name": "Others-create",
      "backtrace_function": ".*blink::[a-zA-Z0-9_]*::create .*"
    }
  ]
}
